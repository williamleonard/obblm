<?xml version="1.0" encoding="UTF-8"?>
<!-- 
    OBBLM team import XML schema 
    Please DO NOT change or delete any XML tag identifiers!
-->
<xmlimport>

    <!-- General team fields -->
    <coach>Coach name</coach>
    <name>Team name</name>
    <race_id>0</race_id>
    <treasury>0</treasury>
    <apothecary>0</apothecary>
    <rerolls>0</rerolls>
    <ff>0</ff> <!-- Use your current TOTAL FF count -->
    <ass_coaches>0</ass_coaches>
    <cheerleaders>0</cheerleaders>
    
    <!-- Statistics -->
    <won>0</won>   <!-- Won matches -->
    <lost>0</lost> <!-- Lost matches -->
    <draw>0</draw> <!-- Draw matches -->
    <wt>0</wt>     <!-- Won tournaments -->
    <gf>0</gf>     <!-- Goals by team -->
    <ga>0</ga>     <!-- Goals against team -->
    <tcas>0</tcas> <!-- Total team CAS -->

    <!-- Players -->
    <!-- To add more players simply add more player tags in the same way as done below -->

        <player>
            <name>Player #1 name</name>
            <position>Linewoman</position>
            <!-- Status may be one of the following: "ready", "mng" or "dead" -->
            <status>ready</status>
            <!-- Stats: cp/td/int/bh/si/ki/mvp -->
            <stats>0/0/0/0/0/0/0</stats>
            <!-- Injuries: ma/st/ag/av/ni -->
            <injs>0/0/0/0/0</injs>
        </player>
        
        <player>
            <name>Player #2 name</name>
            <position>Linewoman</position>
            <status>ready</status>
            <stats>0/0/0/0/0/0/0</stats>
            <injs>0/0/0/0/0</injs>
        </player>
        
</xmlimport>
